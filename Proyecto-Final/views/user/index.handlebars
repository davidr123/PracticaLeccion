Listado de Usuarios
<div class="row">
<a href="/user/new" type="button" class="btn btn-default btn-warning" >Nuevo</a>
</div>
<ul class="list-group">
<div class="row pt-md-5"></div>
{{#each objAdmin}}
<div class="row">
      <br> <br>
    <div class="col-4 col-sm-1">
        
        <a class="list-group-item" href='/user/{{id_administrador}}'>{{id_administrador}},{{usuario}},{{password}},{{rol}}</a>
        </div>
    <div class="col-4 col-sm-1">
        <a type="button" class="btn btn-default btn-danger" href='/user/{{id_administrador}}/edit'>editar</a>
      </div>
    <div class="col-4 col-sm-1">
      <form action='/user/{{id_administrador}}?_method=DELETE' method='post'>
        <button type="submit" class="btn btn-default btn-danger">borrar</button>            
      </form>
    </div>
 <div class="col-6 col-sm-9"></div>
  </div>
{{/each}}
</ul>

  
